{
  "name": "rhd-frontend",
  "version": "1.1.101",
  "description": "Front-end assets and documentation for Red Hat Developer site",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && npm run docs && spandx -c spandx.config.dev.js",
    "test": "karma start",
    "build": "npm run scripts && npm run styles",
    "del": "npm run del:scripts && npm run del:styles",
    "del:scripts": "del \"dist/js**\" && del src/docs/static/js && del docs/js",
    "del:styles": "del \"dist/styles**\" && del src/docs/static/css",
    "fix:fa": "rename -n \"node_modules/@fortawesome/!(fontawesome-common-types)/index.d.ts\" index.es.d.ts",
    "scripts": "npm run del:scripts && npm run fix:fa && npm run scripts:tsc && npm run scripts:old && npm run scripts:copy",
    "scripts:tsc": "tsc && uglifyjs-folder dist/js -c -e -o dist/js",
    "scripts:old": "uglifyjs-folder src/scripts/js -c -o dist/js/rhd.old.min.js",
    "scripts:copyOLD": "copyfiles -u 2 \"dist/js/*.js\" src/docs/static/js",
    "scripts:copy": "npm run scripts:cpmain && npm run scripts:cpnpm",
    "scripts:flatten": "",
    "scripts:cpmain": "copyfiles -u 4 \"dist/js/src/scripts/**/*\" src/docs/static/js && copyfiles -u 2 \"dist/js/*.js\" src/docs/static/js",
    "scripts:cpnpm": "copyfiles -u 3 \"dist/js/node_modules/**/*\" src/docs/static/js",
    "styles": "npm run del:styles && npm run styles:sass && npm run styles:copy",
    "styles:sass": "node-sass src/styles/ -o dist/styles/ && node-sass src/styles/ -o dist/styles-min/ --output-style compressed && postcss -o dist/styles-min/rhd.min.css dist/styles-min/rhd.css",
    "styles:copy": "copyfiles -f \"dist/styles**/*.css\" src/docs/static/css",
    "docs": "hugo -D -s src/docs && npm run docs:front",
    "docs:front": "npm run docs:styles && npm run docs:scripts",
    "docs:styles": "npm run styles:copy && sassdoc \"src/styles/**/*.scss\" -d \"docs/styling\"",
    "docs:scripts": "npm run scripts:copy && jsdoc -c jsdoc.config.json",
    "semantic-release": "semantic-release"
  },
  "repository": "https://github.com/redhat-developer/rhd-frontend.git",
  "author": "<ldary@redhat.com>",
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.15",
    "@fortawesome/pro-light-svg-icons": "^5.7.2",
    "@fortawesome/pro-regular-svg-icons": "^5.7.2",
    "@fortawesome/pro-solid-svg-icons": "^5.7.2",
    "@patternfly/pfe-band": "^1.0.0-prerelease.9",
    "@patternfly/pfe-card": "^1.0.0-prerelease.9",
    "@patternfly/pfe-cta": "^1.0.0-prerelease.9",
    "@patternfly/pfe-datetime": "^1.0.0-prerelease.9",
    "@patternfly/pfe-layouts": "^1.0.0-prerelease.9",
    "@patternfly/pfe-sass": "^1.0.0-prerelease.9",
    "@patternfly/pfe-tabs": "^1.0.0-prerelease.9",
    "@patternfly/pfelement": "^1.0.0-prerelease.9",
    "@rhelements/rh-datetime": "^1.0.0-prerelease.7",
    "@rhelements/rhelement": "^1.0.0-prerelease.7"
  },
  "devDependencies": {
    "browser-sync": "^2.26.3",
    "copyfiles": "^2.1.0",
    "cssnano": "^4.1.10",
    "del-cli": "^1.1.0",
    "eslint": "^5.14.1",
    "hugo-cli": "^0.10.0",
    "jasmine-core": "^3.3.0",
    "jsdoc": "^3.5.5",
    "karma": "^4.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-htmlfile-reporter": "^0.3.8",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-ajax": "^0.1.13",
    "karma-jasmine-html-reporter": "^1.4.0",
    "karma-spec-reporter": "0.0.32",
    "node-sass": "^4.11.0",
    "postcss-cli": "^6.1.1",
    "rename-cli": "^6.0.0",
    "sassdoc": "^2.5.1",
    "semantic-release": "^15.13.3",
    "spandx": "^1.0.3",
    "typescript": "^3.3.3",
    "uglifyjs-folder": "^1.5.1"
  }
}
